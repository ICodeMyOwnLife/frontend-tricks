{
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    { "language": "typescript", "autoFix": true },
    { "language": "typescriptreact", "autoFix": true }
  ],
  "eslint.autoFixOnSave": true,
  "cSpell.words": [
    "abortable",
    "airbnb",
    "buildpack",
    "clsx",
    "codrops",
    "contenteditable",
    "ddpx",
    "devtool",
    "downlevel",
    "dragexit",
    "gitlab",
    "okaidia",
    "pacman",
    "plusplus",
    "predeploy",
    "prismjs",
    "repos",
    "scrollable",
    "unhighlight"
  ],
  "files.exclude": {
    "**/.git": false
  }
}
