{
  "name": "frontend",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.8.2",
    "@material-ui/icons": "^4.5.1",
    "axios": "^0.19.0",
    "brace": "^0.11.1",
    "cb-hooks": "^1.3.1",
    "clsx": "^1.0.4",
    "file-saver": "^2.0.2",
    "json2mq": "^0.2.0",
    "lodash": "^4.17.15",
    "material-design-icons": "^3.0.1",
    "mobile-detect": "^1.4.4",
    "react": "^16.12.0",
    "react-ace": "^8.0.0",
    "react-dom": "^16.12.0",
    "react-hook-form": "^4.2.0",
    "react-media": "^1.10.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-syntax-highlighter": "^11.0.2",
    "typeface-roboto": "^0.0.75",
    "ua-parser-js": "^0.7.20"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/enzyme-to-json": "^1.5.3",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^24.0.23",
    "@types/json2mq": "^0.2.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.15",
    "@types/react-dom": "^16.9.4",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-syntax-highlighter": "^11.0.2",
    "@types/ua-parser-js": "^0.7.33",
    "cz-conventional-changelog": "3.1.0",
    "env-cmd": "^10.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.8.0",
    "eslint-config-react-ts": "^1.3.0",
    "husky": "^4.2.3",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.0",
    "react-scripts": "^3.3.0",
    "source-map-explorer": "^2.1.2",
    "standard-version": "^7.1.0",
    "typescript": "^3.7.3"
  },
  "scripts": {
    "commit": "npx git-cz",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:staging": "env-cmd -f .env.staging react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "deploy:heroku": "git push heroku master",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "type-check": "tsc --noEmit",
    "release": "standard-version && git push --follow-tags"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn type-check && yarn lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && npx git-cz --hook || true"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint:fix",
      "git add"
    ],
    "{*.{json,md}}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
