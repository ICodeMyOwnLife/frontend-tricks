{
  "git": {
    "commitMessage": "chore(release): release ${version}",
    "pushRepo": "origin"
  },
  "github": {
    "release": true,
    "origin": "https://github.com"
  },
  "npm": {
    "publish": false
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "angular",
      "infile": "CHANGELOG.md"
    }
  }
}
